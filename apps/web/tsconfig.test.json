{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "outDir": "./dist-tests",
    "module": "CommonJS",
    "target": "ES2020",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "types": ["node"],
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": [
    "src/app/auth/login/logic.ts",
    "src/lib/api/**/*.ts",
    "tests/**/*.ts",
    "src/lib/constants.ts"
  ]
}
